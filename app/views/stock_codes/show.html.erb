<h3>StockCodes</h3>

<table class="table table-striped table-bordered table-hover">
  <thead>
    <tr>
      <th>종목코드</th>
      <th>이름</th>
      <th>영문이름</th>
    </tr>
  </thead>
  <% @stock_codes.each do |stock_code| %>
      <tr>
        <td><%= stock_code.symbol %></td>
        <td><%= stock_code.name %></td>
        <td><%= stock_code.eng_name %></td>
      </tr>
  <% end %>
</table>
