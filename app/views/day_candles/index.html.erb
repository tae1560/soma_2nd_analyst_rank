<h3>DayCandles</h3>

<table class="table table-striped table-bordered table-hover">
  <thead>
    <tr>
      <th>symbol</th>
      <th>open</th>
      <th>high</th>
      <th>low</th>
      <th>close</th>
      <th>volume</th>
    </tr>
  </thead>
  <% @day_candles.each do |day_candle| %>
      <tr>
        <td><%= day_candle.symbol %></td>
        <td><%= day_candle.open %></td>
        <td><%= day_candle.high %></td>
        <td><%= day_candle.low %></td>
        <td><%= day_candle.close %></td>
        <td><%= day_candle.volume %></td>
      </tr>
  <% end %>
</table>
